<dx:DXWindow x:Class="LayoutControlDemo.Modules.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LayoutControlDemo.Modules"
        xmlns:demo="clr-namespace:DevExpress.Xpf.LayoutControlDemo" 
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
        xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
             
        xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
        xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
        dx:ThemeManager.Theme="MetropolisLight"
        mc:Ignorable="d" Title="Window1" Height="600" Width="1200">

    <Window.Resources>
        <dxmvvm:BooleanNegationConverter x:Key="BooleanNegationConverter"/>
    </Window.Resources>
    <StackPanel VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
        <demo:DataLayout1  Margin="5 0 0 0"
            x:FieldModifier="public"
            x:Name="demo1" >
        </demo:DataLayout1>
        <!--DevExpress.Xpf.LayoutControlDemo.DataLayout1-->
    </StackPanel>
</dx:DXWindow>
